{
  "name": "BitField",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/name/repo.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "babel src --minified -d dist",
    "build:dev": "babel src -s inline -d dist",
    "build:watch": "npm run build:dev -- -w",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha tests --compilers js:babel-register",
    "test:watch": "npm run test -- --reporter min -w",
    "prepublish": "npm run clean & npm run build",
    "run": "npm run lint && node dist"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-config-xo": "^0.15.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-babel": "^3.3.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-es2015-modules-umd"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "xo/esnext",
    "rules": {
      "no-unused-vars": "warn",
      "space-before-function-paren": "off",
      "babel/object-curly-spacing": ["error", "always", { "objectsInObjects": false }]
    },
    "env": {
      "mocha": true
    }
  }
}
